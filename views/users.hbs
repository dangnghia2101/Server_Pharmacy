<script>
    const edit = (id) => {
        window.location.href = `categories/${id}/edit`
    }

    const detail = (id) => {
        window.location.href = `categories/${id}/detail`
    }


    let deleted_id;

    const goDelete = (id) => {
        deleted_id = id;
    }

    const fetchAPI = async (url, option) => {
        const res = await fetch(url, option);
        return res.json();
    }

    const onDelete = async () => {
        const url = `http://192.168.169.1:3000/categories/${deleted_id}/delete`;
        const option = {
            method: 'delete',
            headers: { 'Content-Type': 'application/json' }
        }
        try {
            await fetchAPI(url, option);
            window.location.href = '/categories';
        } catch (e) { console.log('Delete error: ', e) }
    }
</script>